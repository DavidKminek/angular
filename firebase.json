{
  "hosting": {
    "public": "dist/first-app/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "public/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "firestore": {
    "database": "(default)",
    "location": "eur3",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}
